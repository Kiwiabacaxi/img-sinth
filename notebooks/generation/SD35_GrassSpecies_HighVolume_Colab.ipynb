    "# Funções de geração otimizadas\n",
    "def generate_image_with_random_seed(prompt_data, execution_id):\n",
    "    \"\"\"\n",
    "    Gera uma única imagem com seed completamente aleatória\n",
    "    \"\"\"\n",
    "    positive_prompt = prompt_data['positive']\n",
    "    negative_prompt = prompt_data['negative']\n",
    "    \n",
    "    # Parâmetros aleatórios para variabilidade máxima\n",
    "    guidance_scale = random.uniform(*CONFIG['GUIDANCE_RANGE'])\n",
    "    num_steps = random.randint(*CONFIG['STEPS_RANGE'])\n",
    "    seed = random.randint(0, 2**32 - 1)\n",
    "    \n",
    "    # Variações no prompt para mais diversidade\n",
    "    prompt_variations = [\n",
    "        positive_prompt,\n",
    "        positive_prompt + \", natural field variation\",\n",
    "        positive_prompt + \", slight environmental variation\",\n",
    "        positive_prompt + \", authentic pasture condition\",\n",
    "        positive_prompt + \", natural lighting variation\"\n",
    "    ]\n",
    "    \n",
    "    selected_prompt = random.choice(prompt_variations)\n",
    "    \n",
    "    # Geração\n",
    "    with torch.inference_mode():\n",
    "        generator = torch.Generator(device=\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "        generator.manual_seed(seed)\n",
    "        \n",
    "        image = pipe(\n",
    "            prompt=selected_prompt,\n",
    "            negative_prompt=negative_prompt,\n",
    "            num_inference_steps=num_steps,\n",
    "            guidance_scale=guidance_scale,\n",
    "            height=CONFIG['IMAGE_SIZE'],\n",
    "            width=CONFIG['IMAGE_SIZE'],\n",
    "            generator=generator\n",
    "        ).images[0]\n",
    "    \n",
    "    # Metadados\n",
    "    metadata = {\n",
    "        'seed': seed,\n",
    "        'guidance_scale': guidance_scale,\n",
    "        'steps': num_steps,\n",
    "        'prompt_variation': selected_prompt[-50:],  # Últimos 50 chars da variação\n",
    "        'execution_id': execution_id,\n",
    "        'timestamp': datetime.now().isoformat()\n",
    "    }\n",
    "    \n",
    "    return image, metadata\n",
    "\n",
    "def save_to_drive_batch(images_batch, condition_key, species, execution_id):\n",
    "    \"\"\"\n",
    "    Salva um lote de imagens no Google Drive\n",
    "    \"\"\"\n",
    "    # Estrutura: drive/grass_generation_high_volume/species/condition/execution_id/\n",
    "    condition_clean = condition_key.replace('_', '-')\n",
    "    drive_dir = os.path.join(\n",
    "        CONFIG['DRIVE_BASE'],\n",
    "        species,\n",
    "        condition_clean,\n",
    "        execution_id\n",
    "    )\n",
    "    os.makedirs(drive_dir, exist_ok=True)\n",
    "    \n",
    "    saved_files = []\n",
    "    for img_data in images_batch:\n",
    "        image, metadata, filename = img_data\n",
    "        \n",
    "        # Salvar imagem\n",
    "        img_path = os.path.join(drive_dir, filename)\n",
    "        image.save(img_path, quality=95, optimize=True)\n",
    "        \n",
    "        # Salvar metadados\n",
    "        meta_path = os.path.join(drive_dir, filename.replace('.png', '_meta.json'))\n",
    "        with open(meta_path, 'w') as f:\n",
    "            json.dump(metadata, f, indent=2)\n",
    "        \n",
    "        saved_files.append(img_path)\n",
    "    \n",
    "    return saved_files\n",
    "\n",
    "def cleanup_memory():\n",
    "    \"\"\"\n",
    "    Limpeza de memória GPU\n",
    "    \"\"\"\n",
    "    gc.collect()\n",
    "    if torch.cuda.is_available():\n",
    "        torch.cuda.empty_cache()\n",
    "\n",
    "print(\"✓ Funções de geração carregadas\")"
   ]
