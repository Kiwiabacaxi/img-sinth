    "# Sistema de progresso e checkpoint\n",
    "class ProgressTracker:\n",
    "    def __init__(self, execution_id):\n",
    "        self.execution_id = execution_id\n",
    "        self.start_time = time.time()\n",
    "        self.checkpoint_file = os.path.join(CONFIG['LOCAL_TEMP'], f'checkpoint_{execution_id}.json')\n",
    "        self.progress = self.load_checkpoint()\n",
    "        \n",
    "    def load_checkpoint(self):\n",
    "        if os.path.exists(self.checkpoint_file):\n",
    "            with open(self.checkpoint_file, 'r') as f:\n",
    "                return json.load(f)\n",
    "        else:\n",
    "            return {condition: 0 for condition in GRASS_PROMPTS.keys()}\n",
    "    \n",
    "    def save_checkpoint(self):\n",
    "        with open(self.checkpoint_file, 'w') as f:\n",
    "            json.dump(self.progress, f, indent=2)\n",
    "    \n",
    "    def update_progress(self, condition, count):\n",
    "        self.progress[condition] = count\n",
    "        self.save_checkpoint()\n",
    "    \n",
    "    def get_total_progress(self):\n",
    "        return sum(self.progress.values())\n",
    "    \n",
    "    def estimate_remaining_time(self):\n",
    "        elapsed = time.time() - self.start_time\n",
    "        total_completed = self.get_total_progress()\n",
    "        total_target = len(GRASS_PROMPTS) * CONFIG['IMAGES_PER_CONDITION']\n",
    "        \n",
    "        if total_completed > 0:\n",
    "            avg_time_per_image = elapsed / total_completed\n",
    "            remaining_images = total_target - total_completed\n",
    "            return remaining_images * avg_time_per_image\n",
    "        return 0\n",
    "    \n",
    "    def print_status(self):\n",
    "        total_completed = self.get_total_progress()\n",
    "        total_target = len(GRASS_PROMPTS) * CONFIG['IMAGES_PER_CONDITION']\n",
    "        elapsed_mins = (time.time() - self.start_time) / 60\n",
    "        remaining_mins = self.estimate_remaining_time() / 60\n",
    "        \n",
    "        print(f\"\\nüìä PROGRESSO GERAL: {total_completed}/{total_target} ({total_completed/total_target*100:.1f}%)\")\n",
